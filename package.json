{
  "name": "proto-1",
  "version": "1.0.0",
  "devDependencies": {
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "syncpack": "14.0.0-alpha.18",
    "turbo": "^2.5.5"
  },
  "packageManager": "pnpm@10.12.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "check-types": "turbo check-types",
    "deps:fix": "pnpm exec syncpack fix",
    "deps:format": "pnpm exec syncpack format",
    "deps:lint": "pnpm exec syncpack lint",
    "deps:update": "pnpm exec syncpack update --target latest",
    "dev": "turbo dev",
    "format": "prettier . --write --cache",
    "lint": "turbo lint"
  }
}
